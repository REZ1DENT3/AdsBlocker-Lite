{
    "name": "AdsBlocker Lite",
    "version": "3.356",
    "description": "AdsBlocker",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/blocker.min.js"
            ]
        }
    ],
    "options_page": "options/options.html",
    "permissions": [
        "webNavigation",
        "contextMenus",
        "unlimitedStorage",
        "tabs",
        "browsingData",
        "storage",
        "http://*/*",
        "https://*/*",
        "*://*/*",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "js/background.min.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_title": "AdsBlocker Lite",
        "default_icon": "img/icon_world.png"
    }
}