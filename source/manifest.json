{
    "name": "AdsBlocker Lite",
    "version": "0.111",
    "description": "AdsBlocker",
    "manifest_version": 2,
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "js/blocker.min.js"
            ],
            "run_at": "document_start",
            "all_frames": true
        }
    ],
    "options_page": "options/options.html",
    "permissions": [
        "storage",
        "activeTab",
        "notifications",
        "webRequest",
        "webRequestBlocking",
        "tabs",
        "webNavigation",
        "contextMenus",
        "unlimitedStorage",
        "browsingData",
        "<all_urls>"
    ],
    "incognito": "split",
    "homepage_url": "https://ads.babichev.net",
    "background": {
        "scripts": [
            "js/background.min.js"
        ]
    },
    "browser_action": {
        "default_title": "AdsBlocker Lite",
        "default_icon": "img/icon_world.png"
    }
}